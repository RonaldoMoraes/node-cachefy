{
  "name": "cachefy",
  "version": "1.0.0",
  "description": "A flexible, type-safe caching abstraction layer for Node.js with support for multiple cache stores",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:docker": "./test-integration.sh",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "lint:commits": "commitlint --from=HEAD~1 --to=HEAD --verbose",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "cache",
    "caching",
    "redis",
    "memcached",
    "memory",
    "typescript",
    "nodejs",
    "performance",
    "observability",
    "modern",
    "esm",
    "async",
    "high-performance"
  ],
  "author": "Ronaldo Moraes <ronaldo.moraes1990@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RonaldoMoraes/node-cachefy.git"
  },
  "bugs": {
    "url": "https://github.com/RonaldoMoraes/node-cachefy/issues"
  },
  "homepage": "https://github.com/RonaldoMoraes/node-cachefy#readme",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "memcached": "^2.2.2",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.37.0",
    "@types/jest": "^29.5.12",
    "@types/memcached": "^2.2.10",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "volta": {
    "node": "20.11.0",
    "pnpm": "8.15.0"
  }
}