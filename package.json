{
  "name": "cachefy",
  "version": "1.0.0",
  "description": "A flexible, type-safe caching abstraction layer for Node.js with support for multiple cache stores",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:docker": "./test-integration.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "cache",
    "caching",
    "redis",
    "memcached",
    "memory",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "memcached": "^2.2.2",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/jest": "^29.5.12",
    "@types/memcached": "^2.2.10",
    "@types/node": "^20.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
